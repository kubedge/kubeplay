[access]
	inheritFrom = All-Projects
[project]
	description = Kubernetes Play
[access "refs/*"]
	owner = group kubedge-admin
	create = group kubedge-admin
	forgeAuthor = group Registered Users
	pushMerge = group kubedge-admin
	editTopicName = group kubedge-admin
	push = group kubedge-admin
[access "refs/for/refs/*"]
	read = group Registered Users
	push = group Registered Users
	rebase = group Registered Users
	submit = group kubedge-admin
	submit = group kubedge-approvers
[access "refs/heads/*"]
	read = group user/kubedge-devops
	create = group user/kubedge-devops
	push = +force group user/kubedge-devops
	pushMerge = group user/kubedge-devops
[access "refs/tags/*"]
	push = group user/kubedge-devops
	push = +force group user/kubedge-devops
